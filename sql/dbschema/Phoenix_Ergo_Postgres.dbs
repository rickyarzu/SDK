		<procedure name="ASSEGNA_PROD_TUTTI_I_CLIENTI   " id="Procedure_eab" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE PROCEDURE ASSEGNA_PROD_TUTTI_I_CLIENTI    declare variable chiave_cliente integer;
