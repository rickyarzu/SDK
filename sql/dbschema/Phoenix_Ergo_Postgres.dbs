		<procedure name="EVASIONE_ORDINI_ACQUISTO       " id="Procedure_1703" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE PROCEDURE EVASIONE_ORDINI_ACQUISTO        declare variable stato_evasione integer;
declare variable tmp_acquisto integer;
declare variable prec_acquisto integer;
declare variable totale integer;
declare variable prodotto integer;
begin
  PREC_ACQUISTO = -1;
  FOR select VOCI_ACQUISTI.PRODOTTO,VOCI_ACQUISTI.ACQUISTO,
             SUM(VOCI_ACQUISTI.QUANTITA) - (SELECT SUM(VOCI_DDT_ENTRANTI.QUANTITA)
                                            FROM VOCI_DDT_ENTRANTI
                                           WHERE VOCI_DDT_ENTRANTI.DDT IN (SELECT REL_DDT_ENTRANTE_ACQUISTI.DDT_ENTRANTE FROM REL_DDT_ENTRANTE_ACQUISTI WHERE REL_DDT_ENTRANTE_ACQUISTI.ACQUISTO = VOCI_ACQUISTI.ACQUISTO)
                                             AND VOCI_DDT_ENTRANTI.PRODOTTO = VOCI_ACQUISTI.PRODOTTO)
     FROM VOCI_ACQUISTI
    GROUP BY VOCI_ACQUISTI.PRODOTTO,VOCI_ACQUISTI.ACQUISTO
    ORDER BY VOCI_ACQUISTI.ACQUISTO
    INTO :PRODOTTO,:TMP_ACQUISTO,:TOTALE
  DO
    BEGIN
      IF(TMP_ACQUISTO <> PREC_ACQUISTO) THEN
         BEGIN
           IF(PREC_ACQUISTO <> -1) THEN
              BEGIN
                IF(((EVASO = 'T') AND (STATO_EVASIONE <> 0)) OR
                   ((EVASO <> 'T') AND (STATO_EVASIONE = 0))) THEN
                   BEGIN
                    ACQUISTO = PREC_ACQUISTO;
                    SUSPEND;
                   END
              END
           STATO_EVASIONE = 1;
           PREC_ACQUISTO = TMP_ACQUISTO;
         END
         IF((TOTALE IS NULL) OR (TOTALE <> 0)) THEN
            STATO_EVASIONE = 0;
        END
   IF(PREC_ACQUISTO <> -1) THEN
     BEGIN
       IF(((EVASO = 'T') AND (STATO_EVASIONE <> 0)) OR
          ((EVASO <> 'T') AND (STATO_EVASIONE = 0))) THEN
          BEGIN
           ACQUISTO = PREC_ACQUISTO;
           SUSPEND;
          END
     END
END]]></string>
		</procedure>
		<procedure name="GET_TAB_PREZZI_CLIENTE         " id="Procedure_257e" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE PROCEDURE GET_TAB_PREZZI_CLIENTE          declare variable mag_importo integer;
declare variable mag_importo_ivato char(1);
begin
  FOR SELECT Prodotti.Chiave,
             Prodotti.Descrizione,
             Prodotti.Iva,
             (Select max(prezzo)
                from Prezzi_x_cliente
               where Prezzi_x_cliente.Cliente = Clienti.Chiave
                 and Prezzi_x_cliente.Prodotto = Prodotti.Chiave),
             (Select max(Prezzo_Ivato)
                from Prezzi_x_cliente
               where Prezzi_x_cliente.Cliente = Clienti.Chiave
                 and Prezzi_x_cliente.Prodotto = Prodotti.Chiave),
             Prodotti.importo,
             Prodotti.importo_ivato
        FROM Prodotti, CLIENTI
       WHERE Clienti.Chiave = :CLIENTE
      ORDER BY Prodotti.Descrizione
      INTO :CHIAVE_PRODOTTO,:DESCRIZIONE,:IVA,:IMPORTO,:IMPORTO_IVATO,:MAG_IMPORTO,:MAG_IMPORTO_IVATO
    DO
      begin
        if(IMPORTO IS NULL) THEN
          BEGIN
             IMPORTO = :MAG_IMPORTO;
             IMPORTO_IVATO = :MAG_IMPORTO_IVATO;
          END
        SUSPEND;
      end
end]]></string>
		</procedure>
		<procedure name="MAX_INTERVENTO_STATINO         " id="Procedure_321" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE PROCEDURE MAX_INTERVENTO_STATINO          declare variable data_controllo date;
declare variable data_ritiro date;
declare variable data_smaltimento date;
declare variable data_restituzione date;
declare variable data_consegna date;
declare variable data_pressatura date;
begin
  FOR SELECT INTERVENTI_ESTINTORI.DATA_CONTROLLO,
             INTERVENTI_ESTINTORI.DATA_RITIRO,
             INTERVENTI_ESTINTORI.DATA_SMALTIMENTO,
             INTERVENTI_ESTINTORI.DATA_RESTITUZIONE,
             INTERVENTI_ESTINTORI.DATA_CONSEGNA
        FROM INTERVENTI_ESTINTORI
       WHERE INTERVENTI_ESTINTORI.STATINO = :CHIAVE_STATINO
        INTO :DATA_CONTROLLO,:DATA_RITIRO,:DATA_SMALTIMENTO,:DATA_RESTITUZIONE,:DATA_CONSEGNA
   DO
       BEGIN
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_CONTROLLO)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_CONTROLLO;
              OPERAZIONE      = 'C';
              ATTREZZATURE    = 'E';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_RITIRO)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_RITIRO;
              OPERAZIONE      = 'R';
              ATTREZZATURE    = 'E';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_SMALTIMENTO)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_SMALTIMENTO;
              OPERAZIONE      = 'S';
              ATTREZZATURE    = 'E';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_RESTITUZIONE)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_RESTITUZIONE;
              OPERAZIONE      = 'Z';
              ATTREZZATURE    = 'E';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_CONSEGNA)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_CONSEGNA;
              OPERAZIONE      = 'G';
              ATTREZZATURE    = 'E';
            END
       END
  FOR SELECT INTERVENTI_ATTREZZATURE.DATA_CONTROLLO,
             INTERVENTI_ATTREZZATURE.DATA_CONSEGNA
        FROM INTERVENTI_ATTREZZATURE
       WHERE INTERVENTI_ATTREZZATURE.STATINO = :CHIAVE_STATINO
        INTO :DATA_CONTROLLO,:DATA_CONSEGNA
   DO
       BEGIN
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_CONTROLLO)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_CONTROLLO;
              OPERAZIONE      = 'C';
              ATTREZZATURE    = 'A';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_CONSEGNA)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_CONSEGNA;
              OPERAZIONE      = 'G';
              ATTREZZATURE    = 'A';
            END
       END
  FOR SELECT INTERVENTI_IMPIANTI_ELETTRICI.DATA_CONTROLLO,
             INTERVENTI_IMPIANTI_ELETTRICI.DATA_CONSEGNA
        FROM INTERVENTI_IMPIANTI_ELETTRICI
       WHERE INTERVENTI_IMPIANTI_ELETTRICI.STATINO = :CHIAVE_STATINO
        INTO :DATA_CONTROLLO,:DATA_CONSEGNA
   DO
       BEGIN
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_CONTROLLO)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_CONTROLLO;
              OPERAZIONE      = 'C';
              ATTREZZATURE    = 'L';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_CONSEGNA)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_CONSEGNA;
              OPERAZIONE      = 'G';
              ATTREZZATURE    = 'L';
            END
       END
  FOR SELECT INTERVENTI_IDRANTI.DATA_CONTROLLO,
             INTERVENTI_IDRANTI.DATA_RITIRO,
             INTERVENTI_IDRANTI.DATA_SMALTIMENTO,
             INTERVENTI_IDRANTI.DATA_RESTITUZIONE,
             INTERVENTI_IDRANTI.DATA_CONSEGNA,
             INTERVENTI_IDRANTI.DATA_PRESSATURA
        FROM INTERVENTI_IDRANTI
       WHERE INTERVENTI_IDRANTI.STATINO = :CHIAVE_STATINO
        INTO :DATA_CONTROLLO,:DATA_RITIRO,:DATA_SMALTIMENTO,:DATA_RESTITUZIONE,:DATA_CONSEGNA,:DATA_PRESSATURA
   DO
       BEGIN
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_CONTROLLO)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_CONTROLLO;
              OPERAZIONE      = 'C';
              ATTREZZATURE    = 'D';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_RITIRO)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_RITIRO;
              OPERAZIONE      = 'R';
              ATTREZZATURE    = 'D';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_SMALTIMENTO)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_SMALTIMENTO;
              OPERAZIONE      = 'S';
              ATTREZZATURE    = 'D';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_RESTITUZIONE)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_RESTITUZIONE;
              OPERAZIONE      = 'Z';
              ATTREZZATURE    = 'D';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_CONSEGNA)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_CONSEGNA;
              OPERAZIONE      = 'G';
              ATTREZZATURE    = 'D';
            END
         IF ((:DATA_OPERAZIONE IS NULL) OR (:DATA_OPERAZIONE < :DATA_PRESSATURA)) THEN
            BEGIN
              DATA_OPERAZIONE = :DATA_PRESSATURA;
              OPERAZIONE      = 'P';
              ATTREZZATURE    = 'D';
            END
       END
  suspend;
end]]></string>
