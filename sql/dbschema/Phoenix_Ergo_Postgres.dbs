		<procedure name="RECORD_LOG_LAVORAZIONI         " id="Procedure_182d" isSystem="false" params_known="n" >
			<string><![CDATA[CREATE PROCEDURE RECORD_LOG_LAVORAZIONI          declare variable chiaveestintore integer;
declare variable vocetrovata smallint;
begin
  VoceTrovata = 0;
  IF (ESTINTORE IS NOT NULL) THEN
      SELECT MAX(CHIAVE)
        FROM ESTINTORI_CLIENTI
       WHERE CHIAVE = :ESTINTORE
       INTO :ChiaveEstintore;
  ELSE
   ChiaveEstintore = NULL;

  SELECT COUNT(*)
    FROM LOG_STATO_LAVORAZIONI
   WHERE DATA = :DATA AND ESTINTORE = :ESTINTORE AND STATINO = :STATINO
   INTO :VoceTrovata;
   if (:VoceTrovata <> 0) then
      BEGIN
        UPDATE LOG_STATO_LAVORAZIONI
           SET DATA          = :DATA,
               OPERAZIONE    = :OPERAZIONE,
               TECNICO       = :TECNICO,
               DESCRIZIONE   = :DESCRIZIONE,
               STATINO       = :STATINO,
               ESTINTORE     = :ChiaveEstintore,
               SPECIFICA     = :SPECIFICA,
               STATO         = :STATO,
               STATINO_ORDINARIO  = :STATINO_ORDINARIO
         WHERE DATA = :DATA AND ESTINTORE = :ESTINTORE AND STATINO = :STATINO;
         VoceTrovata = 1;
      END
   if (VoceTrovata = 0) then
   BEGIN
      INSERT INTO LOG_STATO_LAVORAZIONI
                 (DATA,OPERAZIONE,TECNICO,DESCRIZIONE,STATINO,ESTINTORE,SPECIFICA,STATO,STATINO_ORDINARIO)
          VALUES (:DATA,:OPERAZIONE,:TECNICO,:DESCRIZIONE,:STATINO,:ChiaveEstintore,:SPECIFICA,:STATO,:STATINO_ORDINARIO);
   END
end]]></string>
		</procedure>
