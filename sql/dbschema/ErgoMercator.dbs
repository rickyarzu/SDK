  GROUP BY c.season_id, c.league_id, c.match_day, c.team_id, t.team_name
  ORDER BY c.match_day, (sum(v.points)) DESC, (sum((v.games_scored - v.games_conceded))) DESC, t.team_name;]]></view_script>
