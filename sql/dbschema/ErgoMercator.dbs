    VALUES (nextval('ads.banner_logs_seq'), now(), 0, page_location_id, ip, site_key, site_id, 
            headers);

     
   Return s_banner;
   
 OPEN ref FOR  select image_url, 'http://ads.ergomercator.com/scripts/ergoads.dll&id=' ||  as   banner_url where "number" = rnd;  
 Return ref;
