	p_customer_id, -- anagraph_id
	'Versamento sul conto', --description
	CURRENT_DATE, -- accounting_date
	CURRENT_DATE, -- value_date
	p_amount -- credit
   ); 
   
  RETURN carservice.account_balance(office_id); 
end; 
$function$
;]]></string>
