			<input_param name="p_day" jt="91" type="date" />
			<input_param name="p_slot_id" jt="5" type="int2" />
			<input_param name="p_pickup" jt="-7" type="bool" />
			<input_param name="p_booking_id" jt="-5" type="int8" />
