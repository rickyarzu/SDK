       TEMP_NAME := replace (TEMP_NAME, '?', '');
       TEMP_NAME := replace (TEMP_NAME, '#', '');
       TEMP_NAME := replace (TEMP_NAME, '*', '');
       TEMP_NAME := replace (TEMP_NAME, '?', '');
       TEMP_NAME := replace (TEMP_NAME, ';', '');
