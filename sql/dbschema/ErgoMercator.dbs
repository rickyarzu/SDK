  owngoal smallint,
  lostgoal smallint,
  sent_off smallint,
  injuries smallint,
  minutes smallint,

  SELECT season_id, season_des, season_code, db_schema_id
  FROM football.ft_seasons;
        */
       select football.ft_update_careers (p_season_id, p_league_id) into i; 
 
       return (i);
end; $function$
;]]></string>
