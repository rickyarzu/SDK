			<input_param name="atopology" jt="12" type="varchar" />
			<input_param name="apoint" jt="1111" type="geometry" />
			<input_param name="allowedgesplitting" jt="-7" type="bool" />
			<input_param name="setcontainingface" jt="-7" type="bool" />
		</function>
